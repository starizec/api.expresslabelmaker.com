                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Trial started successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "licence": {
                                            "type": "string",
                                            "example": "unique-licence-uid"
                                        },
                                        "domain": {
                                            "type": "string",
                                            "example": "example.com"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "user@example.com"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Invalid request or licence",
