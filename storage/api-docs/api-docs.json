        "/api/licence/start-trial": {
            "post": {
                "tags": [
                    "Licence"
                ],
                "summary": "Kreira probno razdoblje s neograničenim vremenom i XX brojem izrada naljepnica.",
                "description": "Mora se poslati trial za licenca property ili vraća error.",
                "operationId": "03373878e2555f31c3482a0811cfbf1d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "user": {
                                        "properties": {
                                            "email": {
                                                "type": "string",
                                                "example": "user@example.com"
                                            },
                                            "domain": {
                                                "type": "string",
                                                "example": "example.com"
                                            },
                                            "licence": {
                                                "type": "string",
